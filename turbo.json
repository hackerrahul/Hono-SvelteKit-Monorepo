{
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".svelte-kit/**"]
    },
    "start": {
      "dependsOn": ["^build"]
    },
    "typecheck": {
      "outputs": []
    }
  }
}